{
  "model": "llama3:latest",
  "temperature": 0.2,
  "max_tokens": 4000,
  "base_url": "http://localhost:11434",
  "timeout": 30000,
  "retry_attempts": 3,
  "tools": {
    "parse_pdf": {
      "enabled": true,
      "timeout": 60000
    },
    "normalize_json": {
      "enabled": true,
      "timeout": 30000
    },
    "link_to_supabase": {
      "enabled": true,
      "timeout": 45000
    },
    "run_java_module": {
      "enabled": true,
      "timeout": 120000
    },
    "verify_data": {
      "enabled": true,
      "timeout": 30000
    }
  },
  "logging": {
    "level": "info",
    "file": "logs/ollama-agent.log"
  }
}
