{
  "version": 2,
  "builds": [
    {
      "src": "vofc-viewer/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vofc-viewer/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "vofc-viewer/app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
